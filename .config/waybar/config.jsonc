{
    "position": "top",
    "height": 32,
    "spacing": 5,


    "modules-left": [
        "memory",
        "tray",
    ],
    "modules-center": [
        "hyprland/workspaces",
    ],
    "modules-right": [
        "pulseaudio",
        "network",
        "battery",
        "clock",
        "custom/power",
    ],


    "memory": {
        "format": "{}%  "
    },
    "tray": {
        "spacing": 10
    },


    "hyprland/workspaces": {
    },


    "pulseaudio": {
        "format": "{icon}     {format_source}",
        "format-source": "",
        "format-source-muted": "",
        "format-muted": "󰖁     {format_source}",
        "format-icons": {
            "default": ["", "", "󰕾"]
        },
        "on-click": "pavucontrol",
        "tooltip-format": "{volume}% volume"
    },
    "network": {
        "format-wifi": "󰤨",
        "format-disconnected": "󰤭",
        "tooltip-format": "Connected to {essid}",
        "on-click": "$TERMINAL -a F_window -e sh -c 'sleep 0.1; exec nmtui'"
    },
    "battery": {
        "states": {
            "warning": 20
        },
        "format": "{capacity}%  {icon}",
        "format-icons": [ "󰂎", "󰁺", "󰁻", "󰁼", "󰁽", "󰁾", "󰁿", "󰂀", "󰂁", "󰂂", "󰁹" ],
        "format-charging": "{capacity}%  󰂄",
        "format-warning": "{capacity}%  󰂃",
    },
    "clock": {
        "format": "{:%A %d, %H:%M}"
    },
    "custom/power": {
        "format": "󰐥",
        "on-click": "powermenu"
    }
}
